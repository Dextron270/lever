main := ():
    print("scheduling some timings")
    print("preparing interrupt")
    print("time now", time())
    schedule(timing, 1.0)
    schedule(timing, 0.1)
    schedule(timing, 0.25)
    schedule(timing, 0.5)
    sleep(5.0)
    schedule(timing, 0.1)
    print("some more from main()")

timing = (duration):
    print("entry", duration)
    for k in [0, 1, 2, 3]
        print("tick", sleep(duration), k, duration)
