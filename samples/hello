(print "hello")
(print 1 2 3)
(print true)
(print false)
(print null)
(cond
    (null
     (print "failure"))
    (false
     (print "fail"))
    (true
     (print "first!"))
    (else
     (print "blaa")))
(while false
    (print "never happens"))

(= hello 5)

(print hello)

(= test (func (a b c)
    (print a)
    (print b)
    (print "special" hello)
    (:= hello c)))

(test 1 2 3)
(test 4 5 hello)
